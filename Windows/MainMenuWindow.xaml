<Window x:Class="_163OnMyNeckFeatOgBurmikFtAnisimov.Windows.MainMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_163OnMyNeckFeatOgBurmikFtAnisimov.Windows"
        mc:Ignorable="d"
        Title="MainMenuWindow" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="tbUser"
                   Height="50"
                   Width="150"
                   HorizontalAlignment="Right"
                   Margin="0 0 0 350"/>
        <StackPanel>
            <Button x:Name="BtnEmp"
                    Height="30"
                    Content="Сотрудники"
                    
                    Width="100" Click="BtnEmp_Click"/>
            <Button x:Name="BtnProd"
                    Height="30"
                    Content="продукты"
                    Margin="0 10 0 0"
                    Width="100" Click="BtnProd_Click"/>
            <Button x:Name="BtnAcc"
                    Height="30"
                    Content="Акаунт"
                    Margin="0 10 0 0"
                    Width="100"/>




        </StackPanel>
        
    </Grid>
</Window>
